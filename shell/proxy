onproxy () {
  export http_proxy="http://127.0.0.1:6152"
  export https_proxy="http://127.0.0.1:6152"
  echo "HTTP Proxy on"
}

offproxy() {
  unset http_proxy
  unset https_proxy
  echo "HTTP Proxy off"
}

export http_proxy=http://web-proxy.oa.com:8080
export https_proxy=http://web-proxy.oa.com:8080
export ftp_proxy=http://web-proxy.oa.com:8080
export all_proxy=http://web-proxy.oa.com:8080
